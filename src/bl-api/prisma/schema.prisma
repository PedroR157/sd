model Moves{
  moves_id        Int           @id
  move            String
  version_group_details           String        @unique
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @updatedAt
}

model Move{
  move_id         Int           @id
  name            String        @unique
}

model Version_group_details{
  vgd_id                    Int            @id
	level_learned_at          Int             
	move_learn_method         String         
	version_group             String

}

model Move_learn_method{
  id             Int            @id
  name           String        
}

model Version_group{
  id             Int          @id
  name           String    
} 